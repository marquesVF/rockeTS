import { Field } from 'tsrocket'

{{#if validatorImports}}
import { {{validatorImports}} } from 'class-validator'
{{/if}}

export class {{name}}Dto {

    {{#each fields}}
    @Field({{{this.fieldOptions}}})
    @{{this.validator}}()
    {{this.name}}{{#if this.nullable}}?{{/if}}: {{this.type}}

    {{/each}}
}
