{
  "name": "{{ appName }}",
  "version": "0.0.1",
  "description": "{{ appDescription }}",
  "main": "index.js",
  "author": "{{ appAuthor }}",
  "license": "{{ appLicence }}",
  "scripts": {
    "serve": "node build/server.js",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "dev": "ts-node-dev --respawn --transpileOnly src/server.ts",
    "dev:orm": "TS_NODE_PROJECT=tsconfig.ts-node.json TS_NODE_TRANSPILE_ONLY=true ts-node ./node_modules/typeorm/cli.js"
  },
  "dependencies": {
    "reflect-metadata": "0.1.13",
    "tsrocket": "0.0.6",
    "typeorm": "0.2.24"
  },
  "devDependencies": {
    "sqlite3": "4.1.1",
    "ts-node-dev": "1.0.0-pre.44",
    "typescript": "3.8.3",
    "yarn": "1.22.4"
  },
  "engines": {
    "node": ">=12.14.1"
  }
}
