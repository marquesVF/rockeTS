import { Entity, {{#if columns}}Column, {{/if}}PrimaryGeneratedColumn } from 'typeorm'

@Entity()
export default class {{model}} {

    @PrimaryGeneratedColumn()
    id: number

    {{#each columns}}
    @Column({{#if this.nullable}}{ nullable:true }{{/if}})
    {{this.name}}{{#if this.nullable}}?{{/if}}: {{this.type}}

    {{/each}}
}
